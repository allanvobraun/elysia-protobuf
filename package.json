{
  "name": "elysia-protobuf",
  "author": "Toil",
  "type": "module",
  "version": "1.0.0",
  "description": "Easy support protobuf integration for Elysia. To decode/encode we use @bufbuild/protobuf lib and schemas generated by ts-proto",
  "keywords": [
    "elysia",
    "bun",
    "proto",
    "protobuf",
    "encode",
    "decode",
    "binary",
    "buf"
  ],
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/ilyhalight/elysia-protobuf/issues"
  },
  "homepage": "https://github.com/ilyhalight/elysia-protobuf",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types/*": {
      "require": "./dist/types/*.js",
      "import": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bun": "latest",
    "elysia": "^1.2.22",
    "eslint": "^9.21.0",
    "eslint-plugin-oxlint": "^0.15.12",
    "husky": "^9.1.7",
    "oxlint": "^0.15.12",
    "prettier": "^3.5.2",
    "tsc-esm-fix": "^3.1.2",
    "typescript-eslint": "^8.25.0"
  },
  "peerDependencies": {
    "typescript": "^5",
    "elysia": "^1.0.0"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.3"
  },
  "scripts": {
    "build:bun": "bun run ./scripts/build.ts",
    "lint": "bunx oxlint --ignore-path=.oxlintignore && bunx eslint",
    "prepare": "husky"
  }
}
